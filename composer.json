{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-debugbar": "~2.4",
        "intervention/image": "^2.4",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "3.3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "predis/predis": "^1.1",
        "samjoyce777/laravel-static-map": "^0.2.0",
        "stevebauman/location": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Helpers.php"  
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "/opt/php71/bin/php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "/opt/php71/bin/php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "/opt/php71/bin/php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "/opt/php71/bin/php artisan optimize"
        ]
    },
    "config": {
        "platform": {
            "php" : "7.1.12"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
